# mount point   device name   device mapper path   device mapper name   file sysystem   protect type
/dbdata /dev/block/stl10 /dev/mapper/dbdata dbdata rfs PROTECTED_WITH_PASSWORD 
/data /dev/block/mmcblk0p2 /dev/mapper/data data rfs PROTECTED_WITH_PASSWORD 
/sdcard /dev/block/mmcblk0p1 /dev/mapper/sdcard sdcard vfat PROTECTED_WITH_PASSWORD 
#/ext_sdcard /dev/block/mmcblk1 /dev/mapper/extsdcard sdcard1 vfat PROTECTED_WITH_PASSWORD 
/efs /dev/block/stl3 /dev/mapper/efs efs rfs NOT_PROTECTED
